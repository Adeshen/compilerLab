FUNCTION add :
PARAM temp
t3 := #0 * #4
t4 := &temp
t2 := t4 + t3
t6 := #1 * #4
t7 := &temp
t5 := t7 + t6
t8 := *t2
t9 := *t5
t1 := t8 + t9
RETURN t1

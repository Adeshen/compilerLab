FUNCTION add :
PARAM temp
t3 := #0 * #4
t4 := *temp
t2 := t4 + t3
t6 := #1 * #4
t7 := *temp
t5 := t7 + t6
t8 := *t2
t9 := *t5
t1 := t8 + t9
RETURN t1
FUNCTION main :
DEC op 8
DEC r 16
i := #0
j := #0
LABEL label1 :
IF i < #2 GOTO label2
GOTO label3
LABEL label2 :
LABEL label4 :
IF j < #2 GOTO label5
GOTO label6
LABEL label5 :
t10 := i + j
t12 := j * #4
t13 := &op
t11 := t13 + t12
*t11 := t10
t14 := j + #1
j := t14
GOTO label4
LABEL label6 :
ARG op
t15 := CALL add
t18 := #0 * #8
t17 := t17 + t18
t18 := i * #4
t17 := t17 + t18
t19 := &r
t16 := t19 + t17
*t16 := t15
t22 := #0 * #8
t21 := t21 + t22
t22 := i * #4
t21 := t21 + t22
t23 := &r
t20 := t23 + t21
WRITE t20
t24 := i + #1
i := t24
j := #0
GOTO label1
LABEL label3 :
RETURN #0
